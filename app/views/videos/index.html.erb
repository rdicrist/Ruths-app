<% if @videos.any? %>
<div class="center">
<h1>Latest videos</h1>
</div>
  <% @videos.each do |video| %>
    <center>
    <iframe width="560" height="315" src="https://www.youtube.com/embed/<%= video.uid %>" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
    
    <% if user_signed_in? and current_user.admin%>
      <%= link_to "Delete Video", video_path(video), method: :delete, data: { confirm: "Are you sure you want to delete this post?" } %>
    <%end %>
    </center>
  <% end %>
<% end %>

