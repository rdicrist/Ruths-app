<div class="center">
<h1>Photos</h1>
</div>

<% @images.each do |post| %>
<center>
  <%= link_to (image_tag post.i.url(:medium)), image_path(post) %>
  <p></p>
  <%= post.caption %>
  
  
  
  <% if user_signed_in? and current_user.admin%>
    <%= link_to "Delete Image", image_path(post), method: :delete, data: { confirm: "Are you sure you want to delete this post?" } %>  
  <%end %>
  
 </center>
<% end %>
